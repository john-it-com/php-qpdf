{
    "name": "john-it-com/php-qpdf",
    "description": "A PDF manipulation library based on QPDF",
    "keywords": ["pdf", "qpdf"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tobias Dierich",
            "email": "git@tobiasdierich.de"
        },
        {
            "name": "JOHN IT GmbH",
            "email": "info@john-it.com"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "mikehaertl/php-shellcommand": "^1.5.0",
        "mikehaertl/php-tmpfile": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">9.0",
        "ext-fileinfo": "*"
    },
    "autoload": {
        "psr-4": {
            "tobiasdierich\\qpdf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    }
}
